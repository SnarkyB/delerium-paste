{
  "annotations": { "list": [] },
  "description": "Delerium Paste â€” application, JVM, connection pool, and container metrics",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["delerium"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Delerium Paste",
  "uid": "delerium-paste-v1",
  "version": 1,
  "panels": [

    {
      "type": "stat",
      "title": "Total Pastes Created",
      "id": 1,
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "delerium_pastes_created_total",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "auto"
      },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] } }
      }
    },

    {
      "type": "stat",
      "title": "Active Pastes",
      "id": 2,
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "delerium_active_pastes",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "auto"
      },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 1000 },
            { "color": "red", "value": 5000 }
          ]} }
      }
    },

    {
      "type": "stat",
      "title": "Total Messages Created",
      "id": 3,
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "delerium_messages_created_total",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "auto"
      },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] } }
      }
    },

    {
      "type": "stat",
      "title": "Rate Limit Rejections",
      "id": 4,
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "delerium_rate_limit_rejected_total",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "auto"
      },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green", "value": null },
            { "color": "orange", "value": 10 },
            { "color": "red", "value": 100 }
          ]} }
      }
    },

    {
      "type": "timeseries",
      "title": "Paste & Message Activity",
      "id": 5,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(delerium_pastes_created_total[5m]) * 60",
          "legendFormat": "pastes/min",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(delerium_messages_created_total[5m]) * 60",
          "legendFormat": "messages/min",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(delerium_pastes_deleted_total[5m]) * 60",
          "legendFormat": "deletions/min ({{method}})",
          "refId": "C"
        }
      ],
      "options": { "tooltip": { "mode": "multi", "sort": "desc" } },
      "fieldConfig": { "defaults": { "unit": "short" } }
    },

    {
      "type": "timeseries",
      "title": "HTTP Request Rate",
      "id": 6,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (status) (rate(ktor_http_server_requests_seconds_count[5m])) * 60",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "options": { "tooltip": { "mode": "multi", "sort": "desc" } },
      "fieldConfig": { "defaults": { "unit": "reqpm" } }
    },

    {
      "type": "timeseries",
      "title": "HTTP Latency Percentiles",
      "id": 7,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.50, sum by (le) (rate(ktor_http_server_requests_seconds_bucket[5m])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.95, sum by (le) (rate(ktor_http_server_requests_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.99, sum by (le) (rate(ktor_http_server_requests_seconds_bucket[5m])))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "options": { "tooltip": { "mode": "multi", "sort": "desc" } },
      "fieldConfig": { "defaults": { "unit": "s" } }
    },

    {
      "type": "timeseries",
      "title": "HTTP Error Rate (4xx + 5xx)",
      "id": 8,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(rate(ktor_http_server_requests_seconds_count{status=~\"4..|5..\"}[5m])) * 60",
          "legendFormat": "errors/min",
          "refId": "A"
        }
      ],
      "options": { "tooltip": { "mode": "multi" } },
      "fieldConfig": {
        "defaults": {
          "unit": "reqpm",
          "color": { "mode": "fixed", "fixedColor": "red" }
        }
      }
    },

    {
      "type": "timeseries",
      "title": "JVM Heap Memory",
      "id": 9,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "jvm_memory_used_bytes{area=\"heap\"}",
          "legendFormat": "used",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "jvm_memory_committed_bytes{area=\"heap\"}",
          "legendFormat": "committed",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "jvm_memory_max_bytes{area=\"heap\"}",
          "legendFormat": "max",
          "refId": "C"
        }
      ],
      "options": { "tooltip": { "mode": "multi", "sort": "desc" } },
      "fieldConfig": { "defaults": { "unit": "bytes" } }
    },

    {
      "type": "timeseries",
      "title": "JVM GC Pause Time",
      "id": 10,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(jvm_gc_pause_seconds_sum[5m])",
          "legendFormat": "{{action}} {{cause}}",
          "refId": "A"
        }
      ],
      "options": { "tooltip": { "mode": "multi", "sort": "desc" } },
      "fieldConfig": { "defaults": { "unit": "s" } }
    },

    {
      "type": "timeseries",
      "title": "JVM Threads",
      "id": 11,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "jvm_threads_live_threads",
          "legendFormat": "live",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "jvm_threads_daemon_threads",
          "legendFormat": "daemon",
          "refId": "B"
        }
      ],
      "options": { "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "short" } }
    },

    {
      "type": "timeseries",
      "title": "HikariCP Connections",
      "id": 12,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "hikaricp_connections_active",
          "legendFormat": "active",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "hikaricp_connections_idle",
          "legendFormat": "idle",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "hikaricp_connections_pending",
          "legendFormat": "pending",
          "refId": "C"
        }
      ],
      "options": { "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "short" } }
    },

    {
      "type": "timeseries",
      "title": "Container CPU Usage",
      "id": 13,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 36 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(container_cpu_usage_seconds_total{name=~\".+\"}[5m]) * 100",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ],
      "options": { "tooltip": { "mode": "multi", "sort": "desc" } },
      "fieldConfig": { "defaults": { "unit": "percent" } }
    },

    {
      "type": "timeseries",
      "title": "Container Memory Usage",
      "id": 14,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 36 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "container_memory_usage_bytes{name=~\".+\"}",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ],
      "options": { "tooltip": { "mode": "multi", "sort": "desc" } },
      "fieldConfig": { "defaults": { "unit": "bytes" } }
    },

    {
      "type": "timeseries",
      "title": "Nginx Connections",
      "id": 15,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 44 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "nginx_connections_active",
          "legendFormat": "active",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "nginx_connections_reading",
          "legendFormat": "reading",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "nginx_connections_writing",
          "legendFormat": "writing",
          "refId": "C"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "nginx_connections_waiting",
          "legendFormat": "waiting",
          "refId": "D"
        }
      ],
      "options": { "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "short" } }
    },

    {
      "type": "timeseries",
      "title": "Nginx Request Rate",
      "id": 16,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 44 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(nginx_http_requests_total[5m]) * 60",
          "legendFormat": "requests/min",
          "refId": "A"
        }
      ],
      "options": { "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "reqpm" } }
    }
  ]
}
